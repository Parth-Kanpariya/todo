{
  "name": "backend",
  "version": "1.0.0",
  "description": "rest api for backend project",
  "main": "./src/app.js",
  "scripts": {
    "build": "babel src -d dist",
    "zip": "bestzip ./url-shortner-prod.zip ./src ./.environment ./package.json ./Procfile ./node_modules ./dist",
    "zip:dev": "bestzip ./url-shortner-dev.zip ./src ./.environment ./package.json ./node_modules ./dist",
    "build:prod": "npm run lint && npm run build && npm run zip",
    "build:dev": "npm run lint && npm run build && npm run zip:dev",
    "start": "node -r dotenv/config dist/app.js dotenv_config_path=.environment/prod.env",
    "start:local": "nodemon --watch src/ --exec \"npm run lint && npm run build && node -r dotenv/config\" dist/app.js dotenv_config_path=.environment/example.env",
    "start:dev": "node -r dotenv/config dist/app.js dotenv_config_path=.environment/dev.env",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "./node_modules/.bin/eslint ."
  },
  "author": "Denis Vithani - https://foxbrains.com",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@sendgrid/mail": "^7.4.5",
    "amazon-s3-uri": "^0.1.1",
    "aws-sdk": "^2.1034.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.19.0",
    "cls-rtracer": "^2.6.0",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dns": "^0.2.2",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "express-fileupload": "^1.2.1",
    "express-http-context": "^1.2.4",
    "express-ip": "^1.0.4",
    "express-validator": "^6.10.0",
    "geoip-lite": "^1.4.5",
    "git-last-commit": "^1.0.0",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "link-check": "^5.0.2",
    "lodash": "^4.17.21",
